{
	"api": {
		"streetteamreact": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"defaultAuthentication": {
						"authenticationType": "API_KEY",
						"apiKeyConfig": {
							"apiKeyExpirationDays": 7,
							"description": "provide a valid api key"
						}
					},
					"additionalAuthenticationProviders": []
				}
			}
		}
	},
	"hosting": {
		"amplifyhosting": {
			"service": "amplifyhosting",
			"type": "cicd"
		}
	},
	"function": {
		"SpotifyAPI": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": []
		}
	}
}